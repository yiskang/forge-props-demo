{
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": false
  },
  "globals": {
      "Autodesk": true,
      "THREE": true,
      "SVG": true
  },
  "rules": {
      "indent": [
          "error",
          2,
          {
              "SwitchCase": 1
          }
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ],
      "no-console": 0,
      "no-undef": 1,
      "no-unused-vars": 1,
      "no-extra-boolean-cast": 0
  }
}